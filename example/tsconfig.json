{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "typeRoots": [
      "../dist/"
    ]
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.vue"
  ],
}
